<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    android:layout_centerHorizontal="true"
    android:paddingRight="5dp"
    android:paddingLeft="5dp"
    android:padding="2dp"
    android:orientation="vertical">

    <android.support.v7.widget.GridLayout
        android:id="@+id/idGridlabosis"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@drawable/ima5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <android.support.v7.widget.CardView
            android:id="@+id/cardLabosis"
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:foregroundGravity="center"
            app:cardBackgroundColor="@android:color/transparent">


        <TextView
                android:id="@+id/txtCurso"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
               android:layout_gravity="center_horizontal"
                android:layout_marginLeft="20dp"
                android:text=""
                android:textColor="@color/white"
                android:textSize="25sp"
                android:textStyle="bold"

                />

            <TextView
                android:id="@+id/txtDoc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="96dp"
                android:layout_marginTop="100dp"
                android:text="TextView"
                android:textColor="@color/colorBlanco"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtHoraf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="170dp"
                android:layout_marginTop="60dp"
                android:layout_marginBottom="15dp"
                android:text="TextView"
                android:textColor="@color/colorBlanco"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtHorai2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="60dp"

                android:drawableLeft="@drawable/ic_horas"
                android:text="Horario"
                android:textColor="@color/colorNegro"
                android:textSize="18sp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/txtHor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="60dp"
                android:text="a"
                android:textColor="@color/colorNegro"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtHorai"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:layout_marginLeft="96dp"
                android:layout_marginBottom="15dp"
                android:text="TextView"
                android:textColor="@color/colorBlanco"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtCod"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="236dp"
                android:layout_marginTop="90dp"
                android:text="TextView"
                android:layout_marginBottom="15dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:textColor="@color/colorBlanco"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtCod2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="100dp"
                android:layout_marginLeft="6dp"
                android:drawableLeft="@drawable/ic_profesor4"
                android:text="Docente"
                android:textColor="@color/colorNegro"
                android:textSize="18sp"
                android:layout_marginBottom="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtDoc2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="172dp"
                android:layout_marginTop="100dp"
                android:drawableLeft="@drawable/ic_aula"
                android:text="Aula"
                android:textColor="@color/colorNegro"
                android:textSize="18sp"
                android:layout_marginBottom="15dp"
                android:textStyle="bold" />

        </android.support.v7.widget.CardView>
    </android.support.v7.widget.GridLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints" />
</LinearLayout>